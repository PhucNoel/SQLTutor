<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Faces Servlet</servlet-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Faces Servlet</servlet-name>
		<url-pattern>*.jsf</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>javax.faces.PROJECT_STAGE</param-name>
		<param-value>Development</param-value>
	</context-param>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>primefaces.THEME</param-name>
		<param-value>bootstrap</param-value>
	</context-param>
	<context-param>
		<param-name>primefaces.PRIVATE_CAPTCHA_KEY</param-name>
		<param-value>6LfU2-gSAAAAAN5ovSJs0f7i3FQaZWp7KCPooimH</param-value>
	</context-param>
	<context-param>
		<param-name>primefaces.PUBLIC_CAPTCHA_KEY</param-name>
		<param-value>6LfU2-gSAAAAAOsDQn0-PgiyZ2uAHxg4hThzNnDJ</param-value>
	</context-param>
	<resource-ref>
	    <description>Connection Pool</description>
	    <res-ref-name>jdbc/sqltutorDB</res-ref-name>
	    <res-type>javax.sql.Datasource</res-type>
	    <res-auth>Container</res-auth>
	</resource-ref>
	<resource-ref>
	    <description>Connection Pool</description>
	    <res-ref-name>jdbc/sqltutorUserDB</res-ref-name>
	    <res-type>javax.sql.Datasource</res-type>
	    <res-auth>Container</res-auth>
	</resource-ref>
</web-app>