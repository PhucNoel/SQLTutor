<?xml version="1.0"?>
<ermapping>
  <attribute-map>
    <entry>
      <attribute>Employee.Ssn</attribute>
      <column>employee.ssn</column>
    </entry>
    <entry>
      <attribute>Employee.Fname</attribute>
      <column>employee.first_name</column>
    </entry>
    <entry>
      <attribute>Employee.Minit</attribute>
      <column>employee.middle_initial</column>
    </entry>
    <entry>
      <attribute>Employee.Lname</attribute>
      <column>employee.last_name</column>
    </entry>
    <entry>
      <attribute>Employee.Address</attribute>
      <column>employee.address</column>
    </entry>
    <entry>
      <attribute>Employee.Bdate</attribute>
      <column>employee.birthdate</column>
    </entry>
    <entry>
      <attribute>Employee.Salary</attribute>
      <column>employee.salary</column>
    </entry>
    <entry>
      <attribute>Employee.Sex</attribute>
      <column>employee.sex</column>
    </entry>
    <entry>
      <attribute>Employee.Lname</attribute>
      <column>employee.last_name</column>
    </entry>
    <entry>
      <attribute>Project.Number</attribute>
      <column>project.id</column>
    </entry>
    <entry>
      <attribute>Project.Name</attribute>
      <column>project.name</column>
    </entry>
    <entry>
      <attribute>Project.Location</attribute>
      <column>project.location</column>
    </entry>
    <entry>
      <attribute>Department.Number</attribute>
      <column>department.id</column>
    </entry>
    <entry>
      <attribute>Department.Name</attribute>
      <column>department.name</column>
    </entry>
    <entry>
      <attribute>Dependent.Name</attribute>
      <column>dependent.name</column>
    </entry>
    <entry>
      <attribute>Dependent.Bdate</attribute>
      <column>dependent.birthdate</column>
    </entry>
    <entry>
      <attribute>Dependent.Sex</attribute>
      <column>dependent.sex</column>
    </entry>
    <entry>
      <attribute>Dependent.Relationship</attribute>
      <column>dependent.relationship</column>
    </entry>
    <entry>
    	<attribute>works_on.Hours</attribute>
    	<column>works_on.hours</column>
    </entry>
    <entry>
    	<attribute>manages.Start_date</attribute>
    	<column>department.manager_start_date</column>
    </entry>
  </attribute-map>
  <join-map>
    <entry>
      <relationship>supervision</relationship>
      <join type="foreign_key">
        <keyPair>
          <primary>employee.ssn</primary>
          <foreign>employee.manager_ssn</foreign>
        </keyPair>
      </join>
    </entry>
    <entry>
      <relationship>works_for</relationship>
      <join type="foreign_key">
        <keyPair>
          <primary>department.id</primary>
          <foreign>employee.department_id</foreign>
        </keyPair>
      </join>
    </entry>
    <entry>
      <relationship>manages</relationship>
      <join type="foreign_key">
        <keyPair>
          <primary>employee.ssn</primary>
          <foreign>department.manager_ssn</foreign>
        </keyPair>
      </join>
    </entry>
    <entry>
      <relationship>works_on</relationship>
      <join type="lookup_table">
        <leftKeyPair>
          <primary>employee.ssn</primary>
          <foreign>works_on.employee_ssn</foreign>
        </leftKeyPair>
        <rightKeyPair>
          <primary>project.id</primary>
          <foreign>works_on.project_id</foreign>
        </rightKeyPair>
      </join>
    </entry>
    <entry>
      <relationship>controls</relationship>
      <join type="foreign_key">
        <keyPair>
          <primary>department.id</primary>
          <foreign>project.department_id</foreign>
        </keyPair>
      </join>
    </entry>
    <entry>
      <relationship>dependents_of</relationship>
      <join type="foreign_key">
        <keyPair>
          <primary>employee.ssn</primary>
          <foreign>dependent.employee_ssn</foreign>
        </keyPair>
      </join>
    </entry>
  </join-map>
</ermapping>