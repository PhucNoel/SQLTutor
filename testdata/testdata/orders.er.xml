<?xml version="1.0" encoding="UTF-8"?>
<erdiagram>
	<entities>
		<entity id="entityCustomer">
			<name>Customer</name>
			<attributes>
        		<attribute isKey="true">
          			<name>Number</name>
        		</attribute>
        		<attribute>
        			<name>Name</name>
        		</attribute>
        		<attribute>
        			<name>City</name>
        		</attribute>
      		</attributes>
      		<type>PERSON</type>
    	</entity>
    	<entity id="entityOrder">
      		<name>Order</name>
      		<attributes>
        		<attribute isKey="true">
          			<name>Number</name>
        		</attribute>
        		<attribute>
          			<name>Date</name>
            	</attribute>
            	<attribute>
              		<name>Amount</name>
            	</attribute>
			</attributes>
      		<type>THING</type>
    	</entity>
    	<entity id="entityItem">
	    	<name>Item</name>
	    	<attributes>
	    		<attribute isKey="true">
	    			<name>Number</name>
	    		</attribute>
	    		<attribute>
	    			<name>Unit Price</name>
	    		</attribute>
	    	</attributes>
	    	<type>THING</type>
	    </entity>
    	<entity id="entityWarehouse">
	    	<name>Warehouse</name>
	    	<attributes>
	    		<attribute isKey="true">
	    			<name>Number</name>
	    		</attribute>
	    		<attribute>
	    			<name>City</name>
	    		</attribute>
	    	</attributes>
	    	<type>THING</type>
	    </entity>
	</entities>
	<relationships>
    	<relationship>
      		<name>makes</name>
      		<attributes/>
      		<leftEdge>
        		<entity reference="entityCustomer"/>
        		<constraint>
          			<label>Customer</label>
          			<cardinality>1</cardinality>
        		</constraint>
      		</leftEdge>
      		<rightEdge>
        		<entity reference="entityOrder"/>
		        <constraint>
		          <label>Order</label>
		          <cardinality>N</cardinality>
		        </constraint>
		  	</rightEdge>
    	</relationship>
    	<relationship>
      		<name>contains</name>
      		<attributes>
      			<attribute>
      				<name>Quantity</name>
      			</attribute>
      		</attributes>
      		<leftEdge>
        		<entity reference="entityOrder"/>
		        <constraint>
		          <label>Order</label>
		          <cardinality>N</cardinality>
		        </constraint>
		    </leftEdge>
      		<rightEdge>
		        <entity reference="entityItem"/>
		        <constraint>
		          <label>Item</label>
		          <cardinality>N</cardinality>
		        </constraint>
			</rightEdge>
    	</relationship>
    	<relationship>
    		<name>ships</name>
    		<attributes>
    			<attribute>
    				<name>Shipment Date</name>
    			</attribute>
    		</attributes>
    		<leftEdge>
	    		<entity reference="entityOrder" />
	    		<constraint>
	    			<label>Order</label>
	    			<cardinality>N</cardinality>
	    		</constraint>
	    	</leftEdge>
    		<rightEdge>
	    		<entity reference="entityWarehouse" />
	    		<constraint>
	    			<label>Warehouse</label>
	    			<cardinality>N</cardinality>
	    		</constraint>
	    	</rightEdge>
   	 	</relationship>
  	</relationships>
</erdiagram>