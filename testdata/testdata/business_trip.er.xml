<?xml version="1.0" encoding="UTF-8"?>
<erdiagram>
	<entities>
		<entity id="entitySalesperson">
			<name>Salesperson</name>
			<attributes>
        		<attribute isKey="true">
          			<name>SSN</name>
        		</attribute>
        		<attribute>
        			<name>Name</name>
        		</attribute>
        		<attribute>
        			<name>start_year</name>
          			<dataType>DATETIME</dataType>
        		</attribute>
        		<attribute>
        			<name>department_number</name>
        		</attribute>
      		</attributes>
      		<type>PERSON</type>
    	</entity>
    	<entity id="entityTrip">
      		<name>Trip</name>
      		<attributes>
        		<attribute isKey="true">
          			<name>id</name>
        		</attribute>
        		<attribute>
          			<name>return_date</name>
          			<dataType>DATETIME</dataType>
            	</attribute>
            	<attribute>
              		<name>departure_date</name>
          			<dataType>DATETIME</dataType>
            	</attribute>
            	<attribute>
              		<name>origin</name>
            	</attribute>
            	<attribute>
              		<name>destination</name>
            	</attribute>
			</attributes>
      		<type>THING</type>
    	</entity>
    	<entity id="entityExpense" isWeak="true">
	    	<name>Expense</name>
	    	<attributes>
	    		<attribute isKey="true">
	    			<name>account_id</name>
	    		</attribute>
	    		<attribute>
	    			<name>amount</name>
	    		</attribute>
	    	</attributes>
	    	<type>THING</type>
	    </entity>
	</entities>
	<relationships>
    	<relationship>
      		<name>takes</name>
      		<attributes/>
      		<leftEdge>
        		<entity reference="entitySalesperson"/>
        		<constraint>
          			<label>salesperson</label>
          			<cardinality>1</cardinality>
        		</constraint>
      		</leftEdge>
      		<rightEdge>
        		<entity reference="entityTrip"/>
		        <constraint>
		          <label>Trip</label>
		          <cardinality>N</cardinality>
		        </constraint>
		  	</rightEdge>
    	</relationship>
    	<relationship isIdentifying="true">
      		<name>incurs</name>
      		<attributes/>
      		<leftEdge>
        		<entity reference="entityTrip"/>
		        <constraint>
		          <label>Trip</label>
		          <cardinality>1</cardinality>
		        </constraint>
		    </leftEdge>
      		<rightEdge>
		        <entity reference="entityExpense"/>
		        <constraint>
		          <label>Expense</label>
		          <cardinality>N</cardinality>
		        </constraint>
			</rightEdge>
    	</relationship>
  	</relationships>
</erdiagram>