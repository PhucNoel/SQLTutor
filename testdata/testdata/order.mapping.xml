<?xml version="1.0"?>
<ermapping>
	<attribute-map>
    	<entry>
      		<attribute>Customer.Number</attribute>
      		<column>Customer.cust#</column>
    	</entry>
	    <entry>
	      <attribute>Customer.Name</attribute>
	      <column>Customer.cname</column>
	    </entry>
	    <entry>
	      <attribute>Customer.city</attribute>
	      <column>Customer.city</column>
	    </entry>
	    <entry>
	      <attribute>Order.Number</attribute>
	      <column>Order.order#</column>
	    </entry>
	    <entry>
	      <attribute>Order.Date</attribute>
	      <column>Order.odate</column>
	    </entry>
	    <entry>
	      <attribute>Order.amount</attribute>
	      <column>Order.ord_amt</column>
	    </entry>
	    <entry>
	      <attribute>Item.Number</attribute>
	      <column>Item.item#</column>
	    </entry>
	    <entry>
	      <attribute>Item.unit price</attribute>
	      <column>Item.unit_price</column>
	    </entry>
	    <entry>
	      <attribute>Warehouse.number</attribute>
	      <column>Warehouse.Warehouse#</column>
	    </entry>
	    <entry>
	      <attribute>Warehouse.city</attribute>
	      <column>Warehouse.city</column>
	    </entry>
	    <entry>
	    	<attribute>contains.quantity</attribute>
	    	<column>order_item.quantity</column>
	    </entry>
	    <entry>
	    	<attribute>ships.Shipment Date</attribute>
	    	<column>shipment.ship_date</column>
	    </entry>
  	</attribute-map>
  	<join-map>
    	<entry>
      		<relationship>makes</relationship>
	      	<join type="foreign_key">
		        <keyPair>
		          <primary>customer.cust#</primary>
		          <foreign>order.cust#</foreign>
		        </keyPair>
	      	</join>
	    </entry>
	    <entry>
			<relationship>contains</relationship>
			<join type="lookup_table">
		        <leftKeyPair>
		          	<primary>order.order#</primary>
		          	<foreign>order_item.order#</foreign>
		        </leftKeyPair>
		        <rightKeyPair>
		          	<primary>item.item#</primary>
		          	<foreign>order_item.item#</foreign>
		        </rightKeyPair>
      		</join>
	    </entry>
	    <entry>
			<relationship>ships</relationship>
			<join type="lookup_table">
		        <leftKeyPair>
		          	<primary>order.order#</primary>
		          	<foreign>shipment.order#</foreign>
		        </leftKeyPair>
		        <rightKeyPair>
		          	<primary>warehouse.warehouse#</primary>
		          	<foreign>shipment.warehouse#</foreign>
		        </rightKeyPair>
      		</join>
	    </entry>
  	</join-map>
</ermapping>